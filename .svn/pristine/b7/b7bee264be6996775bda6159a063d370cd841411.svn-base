.TopMenu {
  display: flex;
  align-items: center;
  height: 48px;
  color: rgba(255, 255, 255, 0.65);
  .topMenuContentActive {
    color: #02f3dd;
  }
  .topMenuContent {
    display: flex;
    align-items: center;
    margin-left: 40px;
    &:hover {
      cursor: pointer;
    }
    svg {
      margin: 0 10px;
    }
  }
}
