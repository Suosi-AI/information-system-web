.box {
  padding: 40px;

  .top {
    display: flex;
    justify-content: space-between;
    margin-bottom: 20px;
  }
  .btn {
    text-align: right;
  }
}

.box :global(.ant-table-thead > tr > th) {
  font-weight: bolder;
  font-size: 16px;
  text-align: center;
}
.box :global(.ant-table-tbody > tr > td) {
  text-align: center;
}
