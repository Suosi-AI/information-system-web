.container {
  .timeStyle {
    padding: 10px 0 0 17px;
  }
}

:global(.ant-modal-content) {
  background-color: #232632 !important;
}
:global(.ant-modal-close-x) {
  color: white;
}
:global(.ant-modal-body) {
  color: white;
  border: none;
}
:global(.ant-modal-header) {
  background-color: #232632 !important;
  border: none;
}
.timeStyle :global(.ant-timeline-item) {
  padding-bottom: 0px;
}
.timeStyle :global(.ant-timeline-item-tail) {
  color: white;
  border-left: 0px;
}
